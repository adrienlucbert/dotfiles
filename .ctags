--langmap=vim:+(vimrc)

--regex-html=/id="([A-Za-z0-9_-]+)"/\1/i,id,ids/
--regex-html=/class="([A-Za-z0-9_-]+)"/\1/c,class,classes/

--langdef=markdown
--langmap=markdown:.md
--regex-markdown=/^(#+[ \t ]+.*)/\1/h,heading,headings/

--exclude=coverage
--exclude=node_modules
--exclude=dist
--exclude=*.min.*
--exclude=vendor
--exclude=tags

--sort=no
